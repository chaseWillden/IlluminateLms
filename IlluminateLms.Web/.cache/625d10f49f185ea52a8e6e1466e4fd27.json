ort const archivedCourse = (state: boolean = false, action: any) => {\n    switch (action.type){\n        case ARCHIVE_COURSE:\n            return action.archived;\n        default: \n            return state;\n    }\n}"},"lineCount":null}},"hash":"eac7e60283a174e0e0cd52da230bc318","cacheData":{"env":{}}}